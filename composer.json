{
    "name": "armincms/core",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "files": [
            "src/Plugin/functions.php",
            "src/Option/helpers.php",
            "src/Armin/functions.php"
        ],
        "psr-4": {  
            "Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
            ],
            "aliases": {
                "Image": "Intervention\\Image\\Facades\\Image",
                "Imager": "Core\\Imager\\Facades\\ImagerFacad"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.12.1"
}
